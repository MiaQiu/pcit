# ElevenLabs API Key (primary - best for similar voices)
# Get your API key from https://elevenlabs.io/
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Deepgram API Key (fallback)
# Get your API key from https://console.deepgram.com/
VITE_DEEPGRAM_API_KEY=your_deepgram_api_key_here

# AssemblyAI API Key (fallback)
# Get your API key from https://www.assemblyai.com/dashboard/
VITE_ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Anthropic Claude API Key (for PCIT analysis - used by backend server)
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Backend API URL (defaults to localhost:3001 for development)
VITE_API_BASE_URL=http://localhost:3001/api

# Email Configuration for Coach Alerts
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
COACH_EMAIL=coach@example.com
