{
  "name": "nora-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*",
    "nora-web",
    "nora-mobile"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=nora-web",
    "dev:mobile": "npm run start --workspace=nora-mobile",
    "server": "npm run server --workspace=nora-web",
    "dev:all": "npm run dev:all --workspace=nora-web",
    "build:web": "npm run build --workspace=nora-web",
    "build:core": "npm run build --workspace=@nora/core"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.713.0",
    "@aws-sdk/client-secrets-manager": "^3.713.0",
    "@aws-sdk/s3-request-presigner": "^3.713.0",
    "@google-cloud/storage": "^7.14.0",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "form-data": "^4.0.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.11",
    "react-native-linear-gradient": "^2.8.3"
  }
}
